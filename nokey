local function makeCallback(urlOrFunc, opts)
    -- accepts either a URL string or a function (executed directly)
    opts = opts or {}
    if type(urlOrFunc) == "function" then
        return function()
            if opts.waitForGame then
                repeat task.wait() until game:IsLoaded() and game.Players and game.Players.LocalPlayer
            end
            if opts.setup then
                for k, v in pairs(opts.setup) do
                    getgenv()[k] = v
                end
            end
            -- Track script usage
            local scriptName = opts.name or "Unknown Function"
            local startTime = tick()
            local success, errorMsg = pcall(urlOrFunc)
            if not success then
                warn("Callback function error:", errorMsg)
            else
                trackScriptUsage(scriptName, startTime)
            end
        end
    else
        return function()
            if opts.waitForGame then
                repeat task.wait() until game:IsLoaded() and game.Players and game.Players.LocalPlayer
            end
            if opts.setup then
                for k, v in pairs(opts.setup) do
                    getgenv()[k] = v
                end
            end
            -- Track script usage
            local scriptName = opts.name or "Unknown Script"
            local startTime = tick()
            local success, errorMsg = pcall(function() loadstring(game:HttpGet(urlOrFunc, true))() end)
            if success then
                trackScriptUsage(scriptName, startTime)
            else
                warn("Script load error:", errorMsg)
            end
        end
    end
end

